                            <AbsDimension x="0" y="10"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                            announceOlderAddon()
                    </OnClick>

                </Scripts>
            </Button>
            <Button name="$parentCloseButton" inherits="UIPanelCloseButton">
                <Size>
                    <AbsDimension x="22" y="22"/>
                </Size>
                <Anchors>
                    <Anchor point="TOPRIGHT">
                        <Offset>
                            <AbsDimension x="0" y="0"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                            hideNeedFrameList();
                    </OnClick>
                </Scripts>
            </Button>
        </Frames>
        <Scripts>
            <OnLoad>
                    this:RegisterForDrag("LeftButton");
                    this:SetMovable(1);
                    this:SetUserPlaced(true);
            </OnLoad>
            <OnDragStart>
                    this:StartMoving();
                    this.isMoving = true;
            </OnDragStart>
            <OnDragStop>
                    this:StopMovingOrSizing();
                    this.isMoving = false;
            </OnDragStop>
        </Scripts>
    </Frame>

</Ui>
